{
  "name": "@codesect/stylelint-config",
  "version": "0.1.0",
  "description": "CodeSect shareable config for stylelint",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore \"**/*.js\"",
    "format": "prettier '**/*.{js,json,md}' --write --list-different",
